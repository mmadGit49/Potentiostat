        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ImpedanceSpectroscopy"
            android:parentActivityName=".NewProcedure">

        </activity>
        <activity
            android:name=".DirectPulseVoltammetry"
            android:parentActivityName=".NewProcedure">

        </activity>
        <activity
